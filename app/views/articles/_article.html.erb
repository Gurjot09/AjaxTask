<body>

<hr>           
  <table class="table">
    <thead>
      <tr>
        <th>Title</th>
        <th>Content</th>
        
      </tr>
    </thead>
    <tbody>
      <tr id = "article_<%= article.id%>">
        <td><%= article.title%></td>
        <td><%= article.content%></td>
        <td><%= link_to 'Edit', edit_article_path(article.id), class: "btn btn-success" , remote: :true %></td>
        <td><%= link_to 'Show',article_path(article.id), class: "btn btn-primary  show1"  %></td>
        <td><%= link_to "Destroy" , article, method: :delete, class: "btn btn-danger delete"%></td>

      </tr>
      
    </tbody>
  </table>
</body>